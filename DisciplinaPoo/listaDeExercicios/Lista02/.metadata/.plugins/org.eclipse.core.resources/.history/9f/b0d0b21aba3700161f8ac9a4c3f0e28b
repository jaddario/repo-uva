import java.util.Scanner;

/*
 * Ler um inteiro n e imprimir seus divisores.
 */
public class Lista02Ex09 {
	public static void main(String[] args) {
		Scanner entrada = new Scanner(System.in);
		System.out.println("Digite o numero para saber seus divisores: \n");
		int numeroAnalisado = entrada.nextInt();

		for (int i = 0; i <= numeroAnalisado; i++) {
			if (i % numeroAnalisado == 0) {
				System.out.printf("%d Ã© divisor de %d \n", i, numeroAnalisado);
			}
		}
	}
}
